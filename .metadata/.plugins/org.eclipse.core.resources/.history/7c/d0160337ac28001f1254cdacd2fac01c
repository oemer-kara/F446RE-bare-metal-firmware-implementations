#include "stm32f446xx.h"

#define GPIOA_ENABLE  (1U << 0)
#define PIN5 		    (1U << 5)
#define LED_PIN		PIN5


void custom_sleep() {
	for(int i = 0; i < 10000000; i++){}
}


//B1 USER: the user button is connected to the I/O PC13 (pin 2)


int main(void){

	RCC->AHB1ENR |=  GPIOA_ENABLE;
	GPIOA->MODER |=  (1U << 10);
	GPIOA->MODER &=~ (1U << 11);




	while(1)
	{

	}
}



